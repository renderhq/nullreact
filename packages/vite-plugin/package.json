{
    "name": "@nullreact/vite-plugin",
    "version": "0.1.0",
    "description": "Vite plugin for NullReact",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
